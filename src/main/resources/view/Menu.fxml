<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="com.pl.maksimum.controller.MenuController">
   <menus>
      <Menu mnemonicParsing="false" text="Plik">
         <items>
            <MenuItem fx:id="infoMenuItem" mnemonicParsing="false" text="Info" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" text="Zamknij" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Opcje">
         <items>
            <MenuItem fx:id="settingsMenuItems" disable="true" mnemonicParsing="false" text="Ustawienia" />
         </items>
      </Menu>
   </menus>
</MenuBar>
